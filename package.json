{
  "name": "node_ts_tutorial",
  "version": "1.0.0",
  "description": "http://brianflove.com/2016/11/08/typescript-2-express-node/",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./bin/www",
    "grunt": "grunt",
    "start": "node ./bin/www",
    "copy": "cp -r src/public dist/ && cp -r src/views dist/",
    "clean": "rm -fr dist/*",
    "build_server": "tsc src/server.ts --outDir dist",
    "build": "npm run clean && npm run copy && npm run build_server && tsc -w"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.16.0",
    "cookie-parser": "^1.4.3",
    "errorhandler": "^1.5.0",
    "express": "^4.14.0",
    "method-override": "^2.3.7",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta8"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/errorhandler": "0.0.30",
    "@types/method-override": "0.0.29",
    "@types/morgan": "^1.7.32",
    "nodemon": "^1.11.0",
    "typescript": "^2.1.5",
    "npm-watch": "^0.1.7"
  }
}
